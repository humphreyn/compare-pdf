{
	"arrowParens": "always",
	"bracketSpacing": true,
	"printWidth": 120,
	"quoteProps": "preserve",
	"semi": true,
	"singleQuote": false,
	"tabWidth": 2,
	"trailingComma": "none",
	"useTabs": true,
	"xmlWhitespaceSensitivity": "strict",
	"overrides": [
		{
			"files": "*.md",
			"parser": "markdown"
		}
	]
}
